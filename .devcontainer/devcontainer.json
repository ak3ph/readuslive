{
  "name": "Flutter Web (Codespaces)",
  "image": "ghcr.io/cirruslabs/flutter:stable",
  "forwardPorts": [3000],
  "postCreateCommand": "flutter config --enable-web && flutter pub get",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  }
}